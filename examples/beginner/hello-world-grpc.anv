# yaml-language-server: $schema=../../schemas/v1.0.0.json

Domain: HelloWorld

Usecase:
  Methods:
    HelloWorld:
      Description: |
        Returns a hello world message
      Output:
        Type: Map
        Properties:
          Message:
            Type: String

Delivery:
  Grpc:
    Rpcs:
      - UsecaseMethod: HelloWorld
